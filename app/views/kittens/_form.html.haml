- if @kitten.errors.any? 
    %ul
        - @kitten.errors.full_messages.each do |msg| 
            %li= msg 

= form_for @kitten do |f|
    =f.label :name
    %p=f.text_field :name 
    =f.label :age
    %p=f.number_field :age
    =f.label :cuteness
    %p=f.text_field :cuteness
    =f.label :softness
    %p=f.text_field :softness
    =f.submit 